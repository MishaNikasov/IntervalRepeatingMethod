<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.IntervalRepeatingMethod" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/whiskey</item>
        <item name="colorPrimaryVariant">@color/champagne</item>
        <item name="colorOnPrimary">@color/off_yellow</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#e9edc9</item>
        <item name="colorSecondaryVariant">#ccd5ae</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <!-- Customize your theme here. -->
        <item name="fontFamily">@font/nunito_sans_regular</item>
    </style>
</resources>